@article{Bruno2002,
abstract = {The paper focuses on one of the emerging technologies for constructing$\backslash$na mobile ad hoc network: Bluetooth. Bluetooth can be exploited on$\backslash$nsmall scales, to build ad hoc wireless personal area networks (WPAN),$\backslash$ni.e., networks that connect devices placed inside a circle with radius$\backslash$nof 10 m. The first part of the paper contains a tutorial-oriented$\backslash$ndescription of the Bluetooth architecture and protocols. In addition,$\backslash$nwe extensively discuss the performance of this technology by$\backslash$ninvestigating, through simulation, the main performance figures: the$\backslash$nprotocol efficiency, the response time, and the system power.$\backslash$nSpecifically, the performance analysis focuses on the algorithms for$\backslash$nscheduling the transmissions in a Bluetooth network. The Bluetooth $\backslash$nSpecification indicates a round robin scheduler as possible solution. $\backslash$nFor this reason, we first study in depth the performance figures of a $\backslash$nBluetooth network with a round robin scheduler. Our results point out $\backslash$nthe inefficient Bluetooth behavior under asymmetric traffic conditions.$\backslash$nTo solve this problem we propose and analyze an innovative scheduling $\backslash$nalgorithm specifically tailored to the Bluetooth characteristic. This $\backslash$nalgorithm, named efficient double-cycle (EDC), dynamically adapts the $\backslash$npolling frequency to the traffic conditions. A performance study $\backslash$nindicates that our EDC scheduler always outperforms the round robin $\backslash$nscheduler},
author = {Bruno, Raffaele and Conti, Marco and Gregori, Enrico},
doi = {10.1023/A:1013989524865},
file = {:home/roliveira/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bruno, Conti, Gregori - 2002 - Bluetooth Architecture, Protocols and Scheduling Algorithms.pdf:pdf},
isbn = {1386-7857},
issn = {1386-7857},
journal = {Cluster Computing},
keywords = {ad hoc networks,mac protocol,performance evaluation,scheduling,wireless pans},
number = {2},
pages = {117--131},
publisher = {Kluwer Academic Publishers},
title = {{Bluetooth : Architecture , Protocols and Scheduling Algorithms}},
url = {http://link.springer.com/10.1023/A:1013989524865},
volume = {5},
year = {2002}
}
@article{Rahman2015,
abstract = {ZigBee, Bluetooth LE, Z Wave, NFC, HomePlug GP and Wi-Fi are six protocols standard for short range wireless communications with low power consumption. From an application point of view, ZigBee is designed for reliable wirelessly networked monitoring and control network, Bluetooth is intended for a cordless mouse, keyboard, and hands-free headset, Z-Wave is for home appliances to communicate with one another for the purposes of home automation, and NFC is to establish radio communication with each other by touching the devices together. Furthermore, HomePlug GP specifications target broadband applications such as in-home distribution of low data rate IPTV, gaming, and Internet content, while Wi-Fi is directed at computer-to-computer connections as an extension or substitution of cabled networks. In this paper, we provide a study of these six wireless communication standards, comparing their main features and behaviors in terms of various metrics, including the transmission time, modulation type, and power consumption. It is believed that the comparison presented in this paper would benefit researchers in selecting an appropriate protocol.},
